   "source": [
    "def twosum(nums, target):\n",
    "    if len(nums)<=1:\n",
    "        return False\n",
    "    temp_buff = {}\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i] in temp_buff:\n",
    "            return [temp_buff[nums[i]], i]\n",
    "            print(temp_buff)\n",
    "        else:\n",
    "            temp_buff[target-nums[i]] = i"
   ]
  },
